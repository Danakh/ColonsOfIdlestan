{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Vitest",
        "runtimeExecutable": "node",
        "runtimeArgs": [
          "--inspect-brk"
        ],
        "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
        "args": [
          "run"
        ],
        "cwd": "${workspaceFolder}",
        "autoAttachChildProcesses": true,
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }